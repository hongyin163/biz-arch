        "doc:server": "styleguidist server",
        "doc": "webpack-cli --config ./build/webpack.production.js && styleguidist build",
        "commit": "git add . && git commit -m ",
        "arthur": "npm run build && npm run doc:build",
        "arthur-start": "pm2 startOrRestart ./ecosystem.json --env production",
        "arthur-stop": "pm2 stop biz-ux",
        "arthur-check": ""
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
