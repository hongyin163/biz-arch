{
    "name": "biz-ux",
    "version": "0.1.66",
    "description": "B端通用组件库",
    "main": "lib/index.js",
    "module": "es/index.js",
    "scripts": {
        "dev": "webpack-dev-server --config ./build/webpack.development.js --name ",
        "test": "jest --config ./.jest.js",
        "start": "pm2 startOrRestart ./ecosystem.json --env production",
        "starttest": "pm2 startOrRestart ./ecosystem.json --env production",
        "build": "gulp -f ./build/gulpfile.js && webpack-cli --config ./build/webpack.production.js",
        "build:es": "gulp -f ./build/gulpfile.js",
        "buildtest": "webpack-cli --config ./build/webpack.production.js",
        "doc:dev": "webpack-cli --config ./build/webpack.production.js && styleguidist server",
